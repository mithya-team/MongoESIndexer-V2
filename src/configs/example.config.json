{
    "collection": "CldDoc",
    "index_on_start": true,
    "force_delete": false,
    "aggregation_pipeline": [],
    "batch_size": 100,
    "index_params": {
        "index": "cld_docs_dev",
        "settings": {
            "index.number_of_shards": 4,
            "index.number_of_replicas": 2,
            "analysis": {
                "filter": {
                    "custom_edgegram": {
                        "type": "edgeNGram",
                        "min_gram": "4",
                        "max_gram": "100"
                    },
                    "wordsdelimeter": {
                        "catenate_words": true,
                        "catenate_all": true,
                        "preserve_original": true,
                        "type": "word_delimiter",
                        "catenate_numbers": true
                    },
                    "english_stop_words": {
                        "type": "stop",
                        "stopwords": "_english_",
                        "ignore_case": true
                    }
                },
                "analyzer": {
                    "autocomplete_analyzer": {
                        "filter": [
                            "lowercase",
                            "wordsdelimeter"
                        ],
                        "type": "custom",
                        "tokenizer": "whitespace"
                    },
                    "autosearch": {
                        "filter": [
                            "lowercase"
                        ],
                        "type": "custom",
                        "tokenizer": "whitespace"
                    },
                    "normalize": {
                        "filter": [
                            "lowercase",
                            "asciifolding"
                        ],
                        "type": "custom",
                        "tokenizer": "keyword"
                    }
                }
            }
        },
