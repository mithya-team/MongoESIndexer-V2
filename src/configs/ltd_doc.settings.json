{
    "collection": "LtdDoc",
    "doc_type": "LtdDoc",
    "index_on_start": false,
    "force_delete": false,
    "batch_size": 500,
    "aggregation_pipeline": [
        {
            "$lookup": {
                "from": "Node",
                "localField": "_id",
                "foreignField": "contentId",
                "as": "node",
                "pipeline": [
                    {
                        "$project": {
                            "title": 1
                        }
                    }
                ]
            }
        }
    ],
    "index_params": {
        "index": "ltd_docs_dev",
        "settings": {
            "index.number_of_shards": 4,
            "index.number_of_replicas": 2,
            "index.max_result_window": 250000,
            "analysis": {
                "filter": {
                    "custom_edgegram": {
                        "type": "edgeNGram",
                        "min_gram": "2",
                        "max_gram": "30"
                    },
                    "wordsdelimeter": {
                        "catenate_words": true,
                        "catenate_all": true,
                        "preserve_original": true,
                        "type": "word_delimiter",
                        "catenate_numbers": true
                    }
                },
                "analyzer": {
                    "autocomplete_analyzer": {
                        "filter": [
                            "lowercase",
                            "wordsdelimeter"
                        ],
                        "type": "custom",
                        "tokenizer": "whitespace"
                    },
                    "autosearch": {
                        "filter": [
                            "lowercase"
                        ],
                        "type": "custom",
                        "tokenizer": "whitespace"
                    },
                    "normalize": {
                        "filter": [
                            "lowercase",
                            "asciifolding"
                        ],
                        "type": "custom",
                        "tokenizer": "keyword"
                    }
                }
            }
        },
        "mappings": {
            "dynamic": false,
